CREATE TABLE attributes (
    id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
    name TEXT,
    rank INTEGER AS Int,
    playCount INTEGER AS Int,
    lowResImage TEXT,
    highResImage TEXT
);

insert:
INSERT OR REPLACE INTO attributes(name, rank, playCount, lowResImage, highResImage)
VALUES (?, ?, ?, ?, ?);

lastId:
SELECT last_insert_rowid();